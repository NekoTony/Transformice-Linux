{
  "name": "transformice-linux",
  "version": "1.0.3",
  "productName": "Transformice",
  "description": "Transformice app for linux",
  "main": "main.js",
  "author": "NekoTony",
  "license": "ISC",
  "dependencies": {
    "pretty-bytes": "^2.0.1"
  },
  "devDependencies": {
    "electron-packager": "^9.0.1",
    "electron-prebuilt": "^0.35.2"
  },
  "build": {
    "icon": "icon.png"
  },
  "scripts": {
    "start": "electron main.js",
    "build": "electron-packager . app --platform linux --arch x64 --out dist/",
    "deb64": "electron-installer-debian --src dist/app-linux-x64/ --dest ~/Desktop --arch amd64"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/NekoTony/Transformice-Linux.git"
  }
}
